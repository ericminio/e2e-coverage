{
    "name": "testcafe-webpack-react",
    "version": "1.0.0",
    "description": "",
    "type": "module",
    "scripts": {
        "pretest": "npm run instrument && npm run build && npm run coverage:clean",
        "test": "npm run test:external && npm run test:internal",
        "test:external": "testcafe chrome:headless about",
        "test:internal": "jest --clearCache && jest app",
        "posttest": "nyc report",
        "coverage:clean": "rm -rf .nyc_output && mkdir .nyc_output",
        "prettier:check": "npx prettier --check .",
        "prettier:write": "npx prettier --write .",
        "instrument": "rm -rf .instrumented && nyc instrument app .instrumented",
        "build": "webpack --config webpack.config.js",
        "start": "node app/server/server.js"
    },
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@babel/cli": "^7.24.7",
        "@babel/core": "^7.24.7",
        "@babel/eslint-parser": "^7.24.7",
        "@babel/plugin-transform-react-jsx": "^7.24.7",
        "@babel/preset-env": "^7.24.7",
        "@babel/preset-react": "^7.24.7",
        "@babel/preset-typescript": "^7.24.7",
        "@testing-library/react": "^16.0.0",
        "@types/jest": "^29.5.12",
        "@types/react": "^18.3.1",
        "@types/react-dom": "^18.3.0",
        "babel-loader": "^9.1.3",
        "babelify": "^10.0.0",
        "jest": "^29.7.0",
        "jest-environment-jsdom": "^29.7.0",
        "prettier": "^2.8.4",
        "testcafe": "^3.6.1",
        "webpack": "^5.92.1",
        "webpack-cli": "^5.1.4"
    },
    "dependencies": {
        "express": "^4.19.2",
        "nyc": "^17.0.0",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-router-dom": "^6.23.1"
    }
}
